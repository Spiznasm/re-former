Edit User
<%= form_for @user do |f| %>
<%= f.label :username, "Username:" %>
<%= f.text_field :username %><br>
<%= f.label :email, "Email:" %>
<%= f.email_field :email, placeholder: "example@email.com" %><br>
<%= f.label :password, "Password:" %>
<%= f.password_field :password %><br>
<%= f.submit "Update User" %>
<% end %>