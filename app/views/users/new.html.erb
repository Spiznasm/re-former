<%= form_for @user do |f| %>
<%= f.label :username, "Username:" %>
<%= f.text_field :username %><br>
<%= f.label :email, "Email:" %>
<%= f.email_field :email, :value => "example@email.com" %><br>
<%= f.label :password, "Password:" %>
<%= f.password_field :password %><br>
<%= f.submit "Create User" %>
<% end %>


  
  
